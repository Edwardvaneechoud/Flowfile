    "@types/react-dom": "^18.3.0",
    "lodash": "^4.17.23",
    "lodash-es": "^4.17.23",
    "tmp": "^0.2.5",
    "js-yaml": "^4.1.1",
    "glob": "^11.0.0",
    "vega": "^6.2.0",
    "vega-functions": "^6.1.1",
    "vega-parser": "^7.0.0",
    "vega-view": "^6.0.0",
    "vega-expression": "^6.1.0",
    "vega-interpreter": "^2.2.1",
