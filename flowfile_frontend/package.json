{
  "name": "Flowfile",
  "version": "0.5.6",
  "description": "A tool for designing and executing data flows",
  "main": "main/main.js",
  "scripts": {
    "dev": "node scripts/dev-server.js",
    "dev:web": "vite --config vite.config.mjs",
    "build": "npm run lint && vue-tsc --noEmit && node scripts/build.js && electron-builder --publish=never",
    "build:web": "npm run lint && vue-tsc --noEmit && vite build --config vite.config.mjs",
    "preview:web": "vite preview",
    "lint": "eslint --fix ./src/**/*.{ts,vue}",
    "build:win": "node scripts/build.js && electron-builder --win",
    "build:mac": "node scripts/build.js && electron-builder --mac",
    "build:linux": "node scripts/build.js && electron-builder --linux",
    "build:test": "node scripts/build.js && electron-builder --dir",
    "test": "playwright test tests/app.spec.ts",
    "test:electron": "playwright test tests/app.spec.ts tests/complex-flow.spec.ts",
    "test:web": "playwright test tests/web-flow.spec.ts",
    "test:all": "playwright test"
  },
  "repository": "https://github.com/Edwardvaneechoud/Flowfile",
  "author": {
    "name": "Edwardvaneechoud",
    "url": "https://github.com/Edwardvaneechoud"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@types/lodash": "^4.17.6",
    "@types/node": "^22.0.0",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitejs/plugin-vue": "^5.2.0",
    "@vue/language-server": "^3.2.0",
    "@vue/compiler-sfc": "^3.4.27",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "chalk": "^4.1.2",
    "chokidar": "^3.5.3",
    "concurrently": "^8.2.2",
    "electron": "^36.0.0",
    "electron-builder": "^25.1.8",
    "electron-playwright-helpers": "^1.7.1",
    "eslint": "^8.13.0",
    "eslint-plugin-vue": "^8.7.0",
    "lint-staged": "^16.2.0",
    "playwright": "^1.57.0",
    "prettier": "^3.2.5",
    "sass": "^1.65.1",
    "ts-loader": "^9.5.1",
    "typescript": "~5.6.3",
    "vite": "^6.2.0",
    "vue-eslint-parser": "^8.3.0",
    "vue-tsc": "^2.0.19"
  },
  "dependencies": {
